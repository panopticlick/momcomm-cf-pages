{
  "name": "momcomm_com",
  "version": "1.3.11",
  "description": "MomComm headless content stack for gear reviews, workflows, and venture playbooks.",
  "license": "MIT",
  "type": "module",
  "packageManager": "pnpm@10.6.2",
  "scripts": {
    "build": "cross-env NODE_OPTIONS=\"--no-deprecation --max-old-space-size=8000\" next build",
    "dev": "cross-env NODE_OPTIONS=--no-deprecation next dev",
    "devsafe": "rm -rf .next && cross-env NODE_OPTIONS=--no-deprecation next dev",
    "generate:importmap": "cross-env NODE_OPTIONS=--no-deprecation payload generate:importmap",
    "generate:types": "cross-env NODE_OPTIONS=--no-deprecation payload generate:types",
    "lint": "cross-env NODE_OPTIONS=--no-deprecation next lint",
    "payload": "cross-env NODE_OPTIONS=--no-deprecation payload",
    "payload:linux": "NODE_OPTIONS=--no-deprecation payload",
    "start": "cross-env NODE_OPTIONS=--no-deprecation next start",
    "test": "pnpm run test:int && pnpm run test:e2e",
    "test:e2e": "cross-env NODE_OPTIONS=--no-deprecation pnpm exec playwright test",
    "test:int": "cross-env NODE_OPTIONS=--no-deprecation vitest run --config ./vitest.config.mts",
    "job:tasks": "cross-env NODE_OPTIONS=--no-deprecation tsx src/scripts/processTasks.ts",
    "seed:ai-blocks": "cross-env NODE_OPTIONS=--no-deprecation tsx src/scripts/seed-ai-blocks.ts"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.71.2",
    "@aws-sdk/client-s3": "^3.948.0",
    "@aws-sdk/lib-storage": "^3.948.0",
    "@next/third-parties": "^16.1.0",
    "@payloadcms/db-postgres": "3.68.1",
    "@payloadcms/next": "3.68.1",
    "@payloadcms/richtext-lexical": "3.68.1",
    "@payloadcms/storage-s3": "^3.68.1",
    "@payloadcms/ui": "3.68.1",
    "@radix-ui/react-aspect-ratio": "^1.1.8",
    "@radix-ui/react-separator": "^1.1.8",
    "@radix-ui/react-slot": "^1.2.4",
    "amazon-paapi": "^1.0.7",
    "aws4fetch": "^1.0.20",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cross-env": "^7.0.3",
    "dotenv": "16.4.7",
    "graphql": "^16.8.1",
    "ioredis": "^5.8.2",
    "liquidjs": "^10.24.0",
    "lucide-react": "^0.561.0",
    "next": "15.4.8",
    "nextjs-toploader": "^3.9.17",
    "payload": "3.68.1",
    "react": "19.2.1",
    "react-dom": "19.2.1",
    "rss": "^1.2.2",
    "sharp": "0.34.2",
    "slugify": "^1.6.6",
    "tailwind-merge": "^3.4.0",
    "tailwindcss-animate": "^1.0.7",
    "zod": "^4.2.1"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.3.3",
    "@opennextjs/cloudflare": "1.14.8",
    "@playwright/test": "1.56.1",
    "@tailwindcss/postcss": "^4.1.18",
    "@testing-library/react": "16.3.0",
    "@types/node": "^22.5.4",
    "@types/pg": "^8.16.0",
    "@types/react": "19.2.1",
    "@types/react-dom": "19.2.1",
    "@types/react-select": "^5.0.1",
    "@types/rss": "^0.0.32",
    "@vitejs/plugin-react": "4.5.2",
    "@vitest/coverage-v8": "^3.2.3",
    "autoprefixer": "^10.4.23",
    "eslint": "^9.16.0",
    "eslint-config-next": "15.4.7",
    "jsdom": "26.1.0",
    "pg": "^8.16.3",
    "playwright": "1.56.1",
    "playwright-core": "1.56.1",
    "postcss": "^8.5.6",
    "prettier": "^3.4.2",
    "tailwindcss": "4.1.18",
    "tsx": "^4.21.0",
    "typescript": "5.7.3",
    "vite-tsconfig-paths": "5.1.4",
    "vitest": "3.2.3",
    "wrangler": "4.59.2"
  },
  "engines": {
    "node": "^24",
    "pnpm": "^9 || ^10"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "sharp",
      "esbuild",
      "unrs-resolver"
    ]
  }
}
